{
  "name": "@coinbase/gemini-cdp-chatbot-example",
  "version": "0.1.0",
  "description": "Example chatbot using Gemini with CDP AgentKit",
  "private": true,
  "main": "chatbot.ts",
  "scripts": {
    "start": "tsx chatbot.ts",
    "build": "tsc",
    "lint": "eslint -c .eslintrc.json \"*.ts\"",
    "lint:fix": "eslint -c .eslintrc.json \"*.ts\" --fix",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
    "format:check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\"",
    "clean": "rm -rf dist/*",
    "test": "npm run build && npm run lint"
  },
  "keywords": [
    "coinbase",
    "sdk",
    "crypto",
    "cdp",
    "agentkit",
    "ai",
    "agent",
    "nodejs",
    "typescript",
    "gemini",
    "google",
    "chatbot"
  ],
  "dependencies": {
    "@coinbase/agentkit": "workspace:*",
    "@coinbase/agentkit-gemini": "workspace:*",
    "@google/generative-ai": "^0.21.0",
    "dotenv": "^16.4.5",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  },
  "author": "Coinbase Inc.",
  "license": "Apache-2.0"
}
